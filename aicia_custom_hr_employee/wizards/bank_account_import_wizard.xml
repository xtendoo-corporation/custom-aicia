<odoo>


    <record id="view_import_employee_bank_account_wizard" model="ir.ui.view">
        <field name="name">import.employee.bank.account.wizard.form</field>
        <field name="model">import.employee.bank.account.wizard</field>
        <field name="arch" type="xml">
            <form string="Importar Cuentas Bancarias">
                <group>
                    <field name="file" filename="file_name"/>
                </group>
                <footer>
                    <button string="Importar" type="object" name="action_import_employee_bank_accounts"
                            class="btn-primary"/>
                    <button string="Cancelar" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
